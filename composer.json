{
    "name": "scolib/simple-repository",
    "type": "library",
    "description": "laravel simple repository",
    "keywords": [
        "laravel",
        "scolib",
        "simple-repository"
    ],
    "homepage": "https://github.com/scolib/simple-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "klgd",
            "email": "slice1213@gmail.com",
            "homepage": "https://github.com/klgd",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/database": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Sco\\Repository\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sco\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
